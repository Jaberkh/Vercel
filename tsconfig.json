{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ESNext",
    "module": "NodeNext",
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "frog/jsx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["frog/client"]
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.mtsx"]
}
